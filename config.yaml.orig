baseURL: 'https://eshopbs5.netlify.app/'
languageCode: 'en-us'
title: 'EStore'
author:
  facebook: " https//facebook.com/example"
  twitter: " https://twitter.com/example"
  email: "contact@example.org"
  name: "EStore Company"
  location: Capetown
  phone: (555) 555-5555
  hours: "Mon-Fri: 9:00AM - 6:000PM, ET"

enableGitInfo: true

module:
  mounts:
    - source: "assets"
      target: "assets"
    - source: "static"
      target: "static"
    - source: "node_modules/bootstrap"
      target: "assets/scss/bootstrap"
    - source: "node_modules"
      target: "static/js"
    - source: "node_modules/@fortawesome/fontawesome-free"
      target: "static"
    - source: "node_modules"
      target: "assets/css"
    - source: "node_modules/@fortawesome/fontawesome-free/scss"
      target: "assets/css"

menus:
  main:
    - identifier: "Home"
      name: "Home"
      url: "/"
      weight: 100
    - identifier: "About"
      name: "About"
      url: "/about"
      weight: 110
    - indentifier: "Contact"
      name: "Contact"
      url: "/contact"
      weight: 120
    - identifier: "Shop"
      name: "Shop"
      url: "/shop"
      weight: 200
    - identifier: "Delivery"
      name: "Delivery"
      url: "/delivery"
      weight: 300
    - identifier: "Privacy"
      name: "Privacy"
      url: "/privacy"
      weight: 400
    - identifier: Contact
      name: Contact
      url: /contact
      weight: 500


params:
  color: "#4f46e5"
  copyright: "Copyright &copy; 2024 EStore Company. All Rights Reserved."
  footer:
    - title: About
      url: /about
      content: >
        EStore Company is the world's leading online shop 
        of digitals, consumables, clothing and hardware products.
        From tv's to hammers and more, we have is all. Browse through 
        our collection of products with different variants.
        We supply the world. You live in it.

    - title: Recent Blog Posts
      recents: blog
      recentCount: 7
    - title: Contact Us
      contact: true
